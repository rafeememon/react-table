{
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "rules": {
    "interface-name": [false],
    "object-literal-sort-keys": false,
    "quotemark": [true, "single", "jsx-single"],
    "arrow-parens": false,
    "jsx-wrap-multiline": false,
    "jsx-boolean-value": [true, "never"]
  }
}
